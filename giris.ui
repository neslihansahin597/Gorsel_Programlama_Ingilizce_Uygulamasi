        <property name="styleSheet">
         <string notr="true">/* Ana Giriş Butonu - btn_login */
QPushButton#btn_login {
    /* Soft ama etkileyici bir mavi-mor geçişi */
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, 
                stop:0 #6366F1, stop:1 #8B5CF6);
    
    /* Yazı bembeyaz ve net */
    color: #FFFFFF; 
    
    /* Kenarlık siyah değil, butonun kendi renginde çok ince bir hat */
    border: 1px solid #4F46E5;
    
    border-radius: 12px;
    padding: 12px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 15px;
    font-weight: bold;
}

/* Fare üzerine geldiğinde renkler canlanır */
QPushButton#btn_login:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, 
                stop:0 #4F46E5, stop:1 #7C3AED);
    border: 1px solid #4338CA;
}

/* Tıklandığında derinlik hissi */
QPushButton#btn_login:pressed {
    background-color: #4338CA;
    /* Tıklandığında hafifçe aşağı çökmüş gibi görünmesi için */
    padding-top: 14px;
    padding-bottom: 10px;
}</string>
