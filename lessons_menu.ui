     <property name="styleSheet">
      <string notr="true">/* Dil Bilgisi Butonu - btn_grammar */
QPushButton#btn_grammar {
    /* Arka plan: Yumuşak bir gökyüzü mavisi */
    background-color: #E0F2FE; 
    
    /* Yazı rengi: Derin deniz mavisi (Siyah yerine) */
    color: #0369A1; 
    
    /* Kenarlık: Çok ince ve şeffaf bir mavi hat */
    border: 1px solid rgba(3, 105, 161, 0.2); 
    
    border-radius: 15px;
    padding: 12px 20px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
    font-weight: 600;
}

/* Fare üzerine geldiğinde (Hover) */
QPushButton#btn_grammar:hover {
    /* Renk biraz daha canlanır */
    background-color: #BAE6FD;
    color: #0284C7;
    border: 1px solid #7DD3FC;
}

/* Tıklandığında (Basılma efekti) */
QPushButton#btn_grammar:pressed {
    background-color: #7DD3FC;
    padding-top: 14px; /* Hafif çökme hissi */
    border: none;
}</string>
