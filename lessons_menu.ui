     <property name="styleSheet">
      <string notr="true">/* ButtonBox genel alanı (Siyah kenarlık olmaması için) */
QDialogButtonBox {
    background-color: transparent;
    border: none;
}

/* ButtonBox içindeki TÜM butonların ortak soft hali */
QDialogButtonBox QPushButton {
    border-radius: 10px;
    padding: 8px 20px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 13px;
    font-weight: 600;
    min-width: 80px;
}

/* TAMAM (OK / Save / Yes) Butonu Stili */
/* Qt genellikle kabul edilen butonları mavi/renkli yapar */
QDialogButtonBox QPushButton[text=&quot;OK&quot;], 
QDialogButtonBox QPushButton[text=&quot;&amp;OK&quot;],
QDialogButtonBox QPushButton[text=&quot;Tamam&quot;] {
    background-color: #DBEAFE; /* Soft Mavi */
    color: #1E40AF; 
    border: 1px solid rgba(30, 64, 175, 0.2);
}

QDialogButtonBox QPushButton[text=&quot;OK&quot;]:hover {
    background-color: #BFDBFE;
    border: 1px solid #93C5FD;
}

/* İPTAL (Cancel / No / Close) Butonu Stili */
/* Vazgeçme butonunu daha sönük ve gri tonlarda yapıyoruz */
QDialogButtonBox QPushButton[text=&quot;Cancel&quot;], 
QDialogButtonBox QPushButton[text=&quot;&amp;Cancel&quot;],
QDialogButtonBox QPushButton[text=&quot;İptal&quot;] {
    background-color: #F1F5F9; /* Soft Gri */
    color: #64748B;
    border: 1px solid rgba(100, 116, 139, 0.2);
}

QDialogButtonBox QPushButton[text=&quot;Cancel&quot;]:hover {
    background-color: #E2E8F0;
    color: #475569;
}

/* Butonlara tıklandığında basılma efekti */
QDialogButtonBox QPushButton:pressed {
    padding-top: 10px;
    background-color: #CBD5E1;
}</string>
