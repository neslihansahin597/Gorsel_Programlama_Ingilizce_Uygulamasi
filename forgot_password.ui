   <item>
    <widget class="QPushButton" name="btn_send_link">
     <property name="styleSheet">
      <string notr="true">/* Sıfırlama Bağlantısı Gönder Butonu - btn_send_link */
QPushButton#btn_send_link {
    /* Arka plan: Güçlü ama soft bir indigo/mor gradyanı */
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, 
                stop:0 #6366F1, stop:1 #8B5CF6);
    
    /* Yazı rengi: Net beyaz (Siyah değil) */
    color: #FFFFFF; 
    
    /* Kenarlık: Siyah yerine butonun rengine yakın soft bir mavi */
    border: 1px solid #4F46E5;
    
    border-radius: 12px;
    padding: 12px;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 14px;
    font-weight: bold;
}

/* Fare üzerine geldiğinde (Hover) */
QPushButton#btn_send_link:hover {
    /* Renkler biraz daha derinleşir ve canlanır */
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, 
                stop:0 #4F46E5, stop:1 #7C3AED);
    border: 1px solid #4338CA;
}

/* Tıklandığında (Basılma efekti) */
QPushButton#btn_send_link:pressed {
    background-color: #4338CA;
    /* Tıklama hissi için yazıyı hafifçe aşağı kaydırır */
    padding-top: 14px;
    padding-bottom: 10px;
}</string>
